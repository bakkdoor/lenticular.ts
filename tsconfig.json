{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noLib": false,
        "declaration": true,
        "removeComments": false,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "alwaysStrict": true,
        "strictNullChecks": true
    },
    "exclude": [
        "node_modules",
        "examples"
    ]
}
